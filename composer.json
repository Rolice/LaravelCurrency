{
  "name": "rolice/laravel-currency",
  "description": "Package that uses Yahoo! Finance API to get exchange rates and stores them in a database for further use.",
  "type": "library",
  "version": "1.0",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "rolice@intellisys.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "illuminate/config": ">=5.1",
    "illuminate/support": ">=5.1",
    "illuminate/database": ">=5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "Rolice\\LaravelCurrency\\": "src",
      "Rolice\\LaravelCurrency\\Models\\": "src/Models/",
      "Rolice\\LaravelCurrency\\Commands\\": "src/Commands/",
      "Rolice\\LaravelCurrency\\Exceptions\\": "src/Exceptions/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Rolice\\LaravelCurrency\\Test\\": "tests/" }
  }
}
